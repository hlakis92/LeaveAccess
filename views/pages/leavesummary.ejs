<!doctype html>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <% include ../partials/head %>

</head>
<body>

<div class="container">
    <% include ../partials/topnav %>
    <div class="container-fluid text-center">
        <div class="row content1">
            <div class="col-sm-10 text-left">

                <!-- this will be the beginning of the form that will contain the employee's information -->
                <!--Input for first and last name-->
                <h4 id="incompleteLeaves">Incomplete Leaves</h4>
                <table id="incompleteLeaves-table">

                    <% if (employeeLeaveSummaryData.hasOwnProperty('incomplete') === true) { %>
                        <tr>
                            <th>Claim Number</th>
                            <th>Type</th>
                            <th>Dates</th>
                        </tr>
                        <% employeeLeaveSummaryData['incomplete'].forEach(function(data) { %>

                            <tr>
                                <td><%= data.leave_info_id %></td>
                                <td><%= data.leave_type %></td>
                                <td><%= data.from_date %> - <%= data.to_date %></td>
                            </tr>

                        <% }); %>
                    <% }  else { %>
                        <tr>
                            <td colspan="3" style="text-align: center">No Record Found</td>
                        </tr>
                    <% } %>
                </table>
                <br>
                <br>
                <h4 id="openLeaves">Open Leaves</h4>
                <table id="openLeaves-table">

                    <% if (employeeLeaveSummaryData.hasOwnProperty('open') === true) { %>
                        <tr>
                            <th>Claim Number</th>
                            <th>Type</th>
                            <th>Dates</th>
                        </tr>
                        <% employeeLeaveSummaryData['open'].forEach(function(data) { %>

                            <tr>
                                <td><a href=/claim<%= data.leave_type %>/<%= data.leave_info_id %>><%= data.leave_info_id %></a></td>
                                <td><%= data.leave_type %></td>
                                <td><%= data.from_date %> - <%= data.to_date %></td>
                            </tr>

                        <% }); %>

                    <% }  else { %>
                        <tr>
                            <td colspan="3" style="text-align: center">No Record Found</td>
                        </tr>
                    <% } %>

                    <!--<tr >
                        <td ><a href="#">20190000000</a></td>
                        <td >Continuous</td>
                        <td >06/25/2018 - 09/15/2018</td>
                    </tr>

                    <tr>
                        <td ><a href="#">20190000000</a></td>
                        <td >Intermittent</td>
                        <td >06/25/2018 - 09/15/2018</td>
                    </tr>-->
                </table>


                <br><br>
                <h4 id="closedLeaves">Closed Leaves</h4>
                <table id="closedLeaves-table">

                    <% if (employeeLeaveSummaryData.hasOwnProperty('closed') === true) { %>
                        <tr>
                            <th>Claim Number</th>
                            <th>Type</th>
                            <th>Dates</th>
                        </tr>
                        <% employeeLeaveSummaryData['closed'].forEach(function(data) { %>
                            <tr>
                                <td><%= data.leave_info_id %></td>
                                <td><%= data.leave_type %></td>
                                <td><%= data.from_date %> - <%= data.to_date %></td>
                            </tr>

                        <% }); %>
                    <% }  else { %>
                        <tr>
                            <td colspan="3" style="text-align: center">No Record Found</td>
                        </tr>
                    <% } %>
                </table>

                <br>

                <div class="form-row">
                    <div class="col-md-12">

                    </div>
                </div>
            </div>
            <!--side bar where employee and supervisor info will populate-->
            <div class="col-sm-2 sidenav2">
                <div class="row">
                    <p>
                        <a class="active" href="#">Employee Information</a>
                    </p>
                    <p>
                        <a href="#">Location Information</a>
                    </p>
                    <p>
                        <a href="#">Leave Reason</a>
                    </p>
                    <p>
                        <a href="#">Provider Information</a>
                    </p>
                    <p>
                        <a href="#">Leave Information</a>
                    </p>
                    <p>
                        <a href="#">Create New Leave</a>
                    </p>
                    <br>
                    <!-- <div class="col-sm-12"> -->
                    <div class="well">
                        <p id="addName">First Last</p>
                        <p id="addAddress">100 someplace ave, somewhere, AN, 00000</p>
                        <p id="employeePhoneNumber">Phone Number: </p>
                        <p id="employeeGender">Gender:</p>
                        <p id="addEmail">Email: someone@place.com</p>
                        <p id="employeeStatus">Status:Active</p>
                    </div>
                    <!-- </div> -->
                </div>

                <div class="row">
                    <!-- <div class="col-sm-12"> -->
                    <div class="noteArea">

                    </div>
                    <!-- </div> -->
                </div>

                <br>

                <div class="row">
                    <!-- <div class="col-sm-12"> -->
                    <div class="well sidenav2">
                        <div id="supervisorcontacts">
                            <p id="supervisorName">Supervisor Name:</p>
                            <p id="supervisorPhoneNumber">Supervisor Number:</p>
                            <p id="supervisorEmail">Supervisor Email:</p>
                        </div>
                        <div id="HRcontacts">
                            <p id="supervisorName">Human Resources Name:</p>
                            <p id="supervisorPhoneNumber">Human Resources Number:</p>
                            <p id="supervisorEmail">Human Resources Email:</p>
                        </div>
                        <div id="payrollcontacts">
                            <p id="supervisorName">Payroll/Benefits Name:</p>
                            <p id="supervisorPhoneNumber">Payroll/Benefits Number:</p>
                            <p id="supervisorEmail">Payroll/Benefits Email:</p>
                        </div>
                        <!-- </div> -->
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

</body>
<% include ../partials/footer %>

<script>


</script>
</html>